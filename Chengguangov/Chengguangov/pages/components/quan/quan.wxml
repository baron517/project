<!--quan.wxml-->

    <scroll-view scroll-x="true" class="tab-h" scroll-left="{{scrollLeft}}">

    </scroll-view>
    <swiper class="tab-content" current="{{currentTab}}" duration="300" bindchange="switchTab" 
     style="height: {{clientHeight?clientHeight+'px':'auto'}}">

     <!--swiper.wxml-->
        <swiper-item>
            <scroll-view scroll-y="true" class="scoll-h" style="height: {{clientHeight?clientHeight+'px':'auto'}}">
             
     <!--content-start.wxml-->
           <view class='content'> 
    <!--detail-->
<block wx:for="{{itemArr}}" >
<view  class='datail shadow' wx:if="{{item.b_use_status==0}}">
  <!--tip-->
 <view class='add-box'>
   <view class='address'>
       <view class='yuan-left'></view>
        <view class='yuan-right'></view>
      <text>未使用</text>
     </view>
   </view>
  <!--quan-->
  <view class='quan'>
     <view class='ma'>
     <view class='ma-text'>{{item.b_code}}</view>
     </view>
  </view>
      <view class='address-1'>
        <view class='local-start'>
       <view class='dot-green-small'></view>
       <view class='start-add'>
        <text class='start-add-text'>始发地: {{item.b_start}}</text>
        </view>
        </view>
        <view class='local-start'>
       <view class='dot-red-small'></view>
       <view class='start-add'>
        <text class='start-add-text'>目的地: {{item.b_end}}</text>
        </view>
        </view>
      </view>
      <view class='xiangxi-text'>
       <text> 购买时间：{{item.b_date}}</text>
     </view>
     <view class='xiangxi-text'>
       <text> 到期时间：{{item.b_start_date}}</text>
     </view>
       <view class='xiangxi-text'>
       <view class='shifukuan'> 实际付款：￥{{item.b_tprice}}</view>
     </view>
      </view>
      </block>
      </view>
      
     <!--content-end.wxml-->

            </scroll-view>
        </swiper-item>
      


     <!--swiper.wxml-->
        <swiper-item>
            <scroll-view scroll-y="true" class="scoll-h" style="height: {{clientHeight?clientHeight+'px':'auto'}}">
             
