.mainContain{
  display: flex;
  flex-direction: column;
}

.switchContain{
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-top: 10px;
}

.personRank{
  border-bottom-left-radius: 5px;
  border-top-left-radius: 5px;
  background-color: #fff;
  padding-top: 5px;
  padding-bottom: 5px;
  padding-left: 20px;
  padding-right: 20px;
  font-size: 12px;
  color: #15abc4;
  border-style: solid;
  border-color: #15abc4;
  border-width: 1px;
}

.personRankSelect{
  border-bottom-left-radius: 5px;
  border-top-left-radius: 5px;
  background-color: #15abc4;
  padding-top: 5px;
  padding-bottom: 5px;
  padding-left: 20px;
  padding-right: 20px;
  font-size: 12px;
  color: #fff;
  
}

.teamRank{
  border-bottom-right-radius: 5px;
  border-top-right-radius: 5px;
  background-color: #fff;
  padding-top: 5px;
  padding-bottom: 5px;
  padding-left: 20px;
  padding-right: 20px;
  font-size: 12px;
  color: #15abc4;
  border-style: solid;
  border-color: #15abc4;
  border-width: 1px;
}

.teamRankSelect{
  border-bottom-right-radius: 5px;
  border-top-right-radius: 5px;
  background-color: #15abc4;
  padding-top: 5px;
  padding-bottom: 5px;
  padding-left: 20px;
  padding-right: 20px;
  font-size: 12px;
  color: #fff;

}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 10px;
  align-self: center;
  justify-content: center;
  flex: 2;
}

.userinfo-avatar {
  width: 128px;
  height: 128px;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  margin: 10px
}

.firstView{
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.bankType{
  display: flex;
  flex: 1;
  flex-direction: column;
  position: fixed;
  right: 10px;
  top: 50px;
}

.bankText{
  width: 30px;
  height: 30px;
  background-color: #15abc4;
  border-radius:50px;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  margin-bottom: 5px;
}

.bankTextNormal{
  width: 30px;
  height: 30px;
  background-color: #fff;
  border-radius:50px;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #15abc4;
  border: solid 1px #15abc4;
  margin-bottom: 5px;
}


.listClass{
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 40px;
  align-items: center;
}

.blockClass{
  display: flex;
  flex-direction: column;
}

.divider{
  height: 2px;
  background-color: #f3f3f3
}

.dividerWidth{
  height: 4px;
  background-color: #f3f3f3
}

.headClass{
  width: 20px;
  height: 20px;
  margin-right: 10px;
  margin-left: 10px;
}

.stepCntClass{
  color: #ff8321;
  margin-right: 20px;
}

.rankClass{
  color: #15abc4;
  width: 60px;
  text-align: center;
  display: flex;
  justify-content: center;
}

.bankTypeNone{
  display: none
}



.usermotto {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  background-color: #f1f1f1;
  width: 100%;
  position: fixed;
  bottom:0px;
  padding-top: 5px;
}

.bottomTab{
  display: flex;
  flex: 1;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}




/*  加载更多   */
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
  margin-bottom: 50px;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}

